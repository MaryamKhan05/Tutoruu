{
	"static": {
		"style": {
			"position": "static"
		}
	},
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"grid": {
		"style": {
			"display": "grid"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"w-40": {
		"style": {
			"width": 160
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	},
	"text-red-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(239 68 68 / var(--tw-text-opacity))"
		}
	},
	"text-blue-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(59 130 246 / var(--tw-text-opacity))"
		}
	}
}